include ./module

doctype html
html
	head
		block Subject
		meta(charset='utf-8')
		meta(name='viewport', content='width=device-width, initial-scale=1.0,maximum-scale=1.0, user-scalable=no')
		block Preload
		block CSS
		block JS
	body
		div#Top
			block Top
		div#Jungle
			span#mobile= locals.mobile
			block Jungle
		div#Middle
			block Middle
		div#Bottom
			block Bottom